server {
    listen 80;
    server_name 127.0.0.1; # здесь прописать или IP-адрес или доменное имя сервера
    access_log  /var/log/nginx/example.log;

    location /static/ {
        root /home/ivan/PycharmProjects/django-movie-web/django_movie_web/;
        expires 30d;
    }

    location /media/ {
    root /home/ivan/PycharmProjects/django-movie-web/django_movie_web/;
    expires 30d;
    }

    location / {
        proxy_pass http://127.0.0.1:8000;
        proxy_set_header Host $server_name;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
}